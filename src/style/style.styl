        overflow: hidden;
        height 100%
      for n in (0..100)
        if (n % 5 == 0 )
          progress[value=\"{n}\"] ~ .progress-bg .progress-bar
            width: n*1%;
        if (n == 100)
          progress[value=\"{n}\"] ~ .progress-bg .progress-bar
            &:before, &:after
              display none
