#root {
  absolute(0, 0);
  size(100%, 100%);
  overflow: hidden;
  font-family 'pixel_num'
  .move-container {
    will-change: transform;
    absolute(0, 0);
    overflow: hidden;
    size(100%, 100%);
  }
  #dead {
    background-image userDead_levelAll
    background-image-repeat: no-repeat;
    animation dead 0.4s infinite, move 1.6s ease-in 0.6s;
    animation-fill-mode: forwards;
  }
  .done {
    background-image: none
    animation none
    &:before {
      bang()
    }
  }

  .animated-object {
    absolute(0, auto);
    bottom 0;
    &__ground {
      .animated-object__body {
          absoluteAll(18px, 0, 18px, 0)
          background-repeat repeat-x;
          background-size auto 100%;
          overflow visible
      }
    }

    &__block {
      background-repeat no-repeat;
      background-size 100% 100%;
    }

    &__bonus {
      background-repeat no-repeat;
      background-size 100% 100%;
    }
  }

  .user {
    will-change: transform;
    absolute(0, auto);
    bottom 0;
    background-image-repeat: no-repeat;
    background-size auto 100%;

    &__run {
      background-position: 0 0;
      animation: play 0.7s steps(7) infinite;
    }
  }
}
