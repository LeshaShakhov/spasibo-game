html, body {
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
  overflow: hidden;
  position: relative;
  -moz-text-size-adjust none
  -webkit-text-size-adjust none
  -ms-text-size-adjust none
  font-family 'pixel_num'
}
*
{
  -webkit-user-select: none;
  -moz-user-select: -moz-none;
  -ms-user-select: none;
  user-select: none;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
size(width, height) {
  width width;
  height height;
}

absolute(left, top) {
  position absolute;
  left: left;
  top: top;
}

absoluteAll(left, top, right, bottom) {
  position absolute
  left: left;
  top: top;
  right: right;
  bottom: bottom;
}
displayFlex(flex-direction, align-items,justify-content){
  display: flex;
  flex-direction: flex-direction;
  align-items align-items;
  justify-content: justify-content;
}

boldText(font-size){
  font-size: font-size;
  font-weight: bold;
}

button(width, background, color, border-radius, font-size, padding){
    display: block;
    width: width;
    background: background;
    color: color;
    border-radius: border-radius;
    text-align: center;
    text-decoration: none;
    font-size: font-size;
    padding: padding;
    font-weight bold;
}

link(color)
  text-decoration none
  color color
  border-bottom: 1px solid color;
triangleBefore(link)
  content ''
  display: inline-block;
  size(200px, 100%)
  position absolute
  right 100%
  background-image link
  background-size auto 100%
  background-repeat no-repeat
  background-position-x right;
triangleAfter(link)
  content ''
  display: inline-block;
  size(200px, 100%)
  absolute(100%, 0)
  background-image link
  background-size auto 100%
  background-repeat no-repeat
bang()
  content ''
  display: inline-block;
  size(160px, 137px)
  position absolute
  bottom 50%
  left 50%
  transform translate(-50%, 50%)
  background-image: bang_levelAll;
  background-position: 0 0;
  background-size auto 100%
  animation: bang 0.35s steps(3);
.level-1 {
  background-image bg_level1;
  background-size auto 100%;
  background-repeat repeat-x;
  .user {
    &__run {
      background-image user_levelAll_run;
    }

    &__jump-up {
      background-image user_level1_jump_up;
    }

    &__jump-down {
      background-image user_level1_jump_down;
    }
  }
  .animated-object__finish {
    background-image finish_level1
    background-size 100% auto
    background-repeat no-repeat
    animation: bonus 1.5s infinite ease-out;
  }
  .animated-object {
    &__ground {
      .animated-object__body {
        background-image ground_level1
        &:before {
          triangleBefore(ground_triangleLeft_level1);
        }
        &:after {
          triangleAfter(ground_triangleRight_level1)
        }
      }
    }

    &__block {
      background-image block_level1
    }

    &__bonus {
      background-image bonus_level1
      animation: bonus 1.5s infinite ease-out;
    }
  }
  .completed {
    background-image: none
    animation none
    &:before {
      bang()
    }
  }
}



